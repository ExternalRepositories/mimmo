<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- xml interface to replicate .cpp example : geohandlers_example_00002.cpp -->
<mimmoXML version="1">
	<Geom0>
		<ClassName>mimmo.Geometry</ClassName>
        <IOMode>2</IOMode>
		<ReadDir>./geodata</ReadDir>
		<ReadFilename>sphere2</ReadFilename>
		<ReadFileType>STL</ReadFileType>
		<WriteDir>./</WriteDir>
		<WriteFilename>geohandlers_output_00002.0000</WriteFilename>
		<WriteFileType>STL</WriteFileType>
	</Geom0>
   <Geom1>
       <ClassName>mimmo.Geometry</ClassName>
        <IOMode>2</IOMode>
       <ReadDir>./geodata</ReadDir>

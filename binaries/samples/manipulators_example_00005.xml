<?xml version="1.0" encoding="ISO-8859-1"?>
<!-- xml interface to replicate .cpp example : manipulators_example_00005.cpp -->
<mimmoXML version="-1">
	<Geom0>
		<ClassName>mimmo.Geometry</ClassName>
		<ReadFlag>1</ReadFlag>
		<ReadDir>./geodata</ReadDir>
		<ReadFilename>sphere2</ReadFilename>
		<ReadFileType>STL</ReadFileType>
		<WriteFlag>1</WriteFlag>
		<WriteDir>./</WriteDir>
		<WriteFilename>manipulators_output_00005.0000</WriteFilename>
		<WriteFileType>STL</WriteFileType>
	</Geom0>
	<Geom1>
		<ClassName>mimmo.Geometry</ClassName>
		<ReadFlag>0</ReadFlag>
		<WriteFlag>1</WriteFlag>
		<WriteDir>./</WriteDir>
		<WriteFilename>manipulators_output_00005.0001</WriteFilename>
		<WriteFileType>STL</WriteFileType>
	</Geom1>
   <InputNode>
       <ClassName>mimmo.GenericInput</ClassName>
       <ReadFromFile>1</ReadFromFile>
       <CSV>1</CSV>
       <ReadDir>input</ReadDir>
       <Filename>manipulators_input_00005n.csv</Filename>
   </InputNode>
	<InputDispl>
		<ClassName>mimmo.GenericInput</ClassName>
       <ReadFromFile>1</ReadFromFile>
       <CSV>1</CSV>
       <ReadDir>input</ReadDir>
		<Filename>manipulators_input_00005d.csv</Filename>
	</InputDispl>
   <MRBF>
       <ClassName>mimmo.MRBF</ClassName>
       <SupportRadius>0.25</SupportRadius>
       <PlotInExecution>1</PlotInExecution>
       <Mode>0</Mode>
   </MRBF>
	<Applier>
		<ClassName>mimmo.Apply</ClassName>
		<RefreshGeometryTrees>1</RefreshGeometryTrees>
	</Applier>
	<Connections>
		<c0>
			<sender>Geom0</sender>
			<senderPort>M_GEOM</senderPort>
			<receiver>MRBF</receiver>
			<receiverPort>M_GEOM</receiverPort>
		</c0>
		<c1>
			<sender>Geom0</sender>
			<senderPort>M_GEOM</senderPort>
			<receiver>Applier</receiver>
			<receiverPort>M_GEOM</receiverPort>
		</c1>
       <c2>
           <sender>InputNode</sender>
           <senderPort>M_COORDS</senderPort>
           <receiver>MRBF</receiver>
           <receiverPort>M_COORDS</receiverPort>
       </c2>
       <c3>
           <sender>InputDispl</sender>
           <senderPort>M_DISPLS</senderPort>
           <receiver>MRBF</receiver>
           <receiverPort>M_DISPLS</receiverPort>
       </c3>
		<c4>
			<sender>MRBF</sender>
			<senderPort>M_GDISPLS</senderPort>
			<receiver>Applier</receiver>
			<receiverPort>M_GDISPLS</receiverPort>
		</c4>
		<c5>
			<sender>Applier</sender>
			<senderPort>M_GEOM</senderPort>
			<receiver>Geom1</receiver>
			<receiverPort>M_GEOM</receiverPort>
		</c5>
	</Connections>
</mimmoXML>	
